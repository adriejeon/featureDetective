# Flask 설정
SECRET_KEY=your-secret-key-here
FLASK_ENV=development
FLASK_DEBUG=1

# 데이터베이스 설정
DATABASE_URL=postgresql://postgres:password@localhost:5432/feature_detective

# Celery 설정
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# 크롤링 설정
CRAWLING_DELAY=1
CRAWLING_TIMEOUT=30
MAX_RETRIES=3

# 로깅 설정
LOG_LEVEL=INFO
